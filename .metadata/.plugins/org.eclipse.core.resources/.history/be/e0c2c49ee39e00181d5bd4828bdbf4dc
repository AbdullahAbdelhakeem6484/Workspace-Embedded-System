#define F_CPU 12000000
#include <avr/io.h>
#include <util/delay.h>

char arr[]={0b10100000,0b11111001,0b11000100,0b11010000,0b10011001,0b10010010,0b10000010,0b11111000,0b10000000,0b10010000};
int main (void)
{
	int i=0;
	DDRA=0xff;

while(1)
{
	for(i=0;i<10;i++)
	{
		PORTA=arr[i];
		_delay_ms(1000);
	}
   }
	return 0;
}

