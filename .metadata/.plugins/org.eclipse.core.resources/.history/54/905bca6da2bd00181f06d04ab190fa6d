#include"STD_Types.h"
#include"macros.h"
#include "LCD_interface.h"

int main (void)
{
	u32 freq=0;
	u32 duty=0;

LCD_voidInit();
TCCR1A=0;
CLEAR_BIT(TCCR1B,4);
CLEAR_BIT(TCCR1B,5);

SET_BIT(TCCR1B,6);


CLEAR_BIT(TCCR1B,2);
CLEAR_BIT(TCCR1B,1);
SET_BIT(TCCR1B,0);

SET_BIT(TIMSK,5);
SET_BIT(TIMSK,2);

SET_BIT(SREG,7);

DIO_voidSetPinDir(PORT_D,PIN_6,OUTPUT);

LCD_voidWriteString("freq= ");
LCD_voidwriteNumber(freq,1,6);
LCD_voidwriteNumber(freq,1,6);
LCD_voidSetCursor(2,0);
LCD_voidWriteString("duty= ");
while(1)
{

}



return 0;
}
