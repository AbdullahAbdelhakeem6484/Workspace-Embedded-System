#include"STD_Types.h"
#define F_CPU 12000000
#include<util/delay.h>
#include<avr/io.h>


int main (void)
{
	u16 i;
	arr_7seg[]={0b10100000,0b11111001,0b11000100,0b11010000,0b10011001,0b10010010,0b10000010,0b11111000,0b10000000,0b10010000};

	DDRA=0XFF;
	DDRD=0XFF;
	DDRB=0XFF;
while(1)
{
	for(i=0;i<10;i++)
	{
		PORTA=arr_7seg[i];
		_delay_ms(1000);
		PORTA=0bff;


	}



}

return 0;
}

