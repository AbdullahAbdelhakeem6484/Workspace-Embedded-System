#include"STD_Types.h"
#include"macros.h"
#include "DIO_interface.h"
#include "SPI_interface.h"

#include<avr/io.h>
#include<avr/interrupt.h>
#define F_CPU 12000000
#include<util/delay.h>

//full doublex
//if u are master

int main(void)
{
	SPI_voidMasterInit();
	SPI_voidSlaveSelect();



while(1)
{
	SPI_u8Transfer('b');
	_delay_ms(1000);

}



return 0;
}









