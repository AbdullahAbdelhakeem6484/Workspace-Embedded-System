#include"macros.h"
#include"STD_Types.h"
#include<avr/io.h>
#include"DIO_interface.h"


#include<util/delay.h>
#define F_CPU 12000000


int main(void)
{
	u32 i;

	DIO_voidSetPinDir(PORT_B,PIN_3,OUTPUT);
while(1)
{

for( i=0;i<250;i++)
{
		DIO_voidSetPinValue(PORT_B,PIN_3,HIGH);
		_delay_us(50);
		DIO_voidSetPinValue(PORT_B,PIN_3,LOW);
		_delay_us(19950);

}
for( i=0;i<250;i++)
{
		DIO_voidSetPinValue(PORT_B,PIN_3,HIGH);
		_delay_us(500);
		DIO_voidSetPinValue(PORT_B,PIN_3,LOW);
		_delay_us(19500);

}
for(i=0;i<250;i++)
{
		DIO_voidSetPinValue(PORT_B,PIN_3,HIGH);
		_delay_us(1500);
		DIO_voidSetPinValue(PORT_B,PIN_3,LOW);
		_delay_us(18500);

}
for( i=0;i<250;i++)
{
		DIO_voidSetPinValue(PORT_B,PIN_3,HIGH);
		_delay_us(500);
		DIO_voidSetPinValue(PORT_B,PIN_3,LOW);
		_delay_us(19500);

}

}



return 0;
}

