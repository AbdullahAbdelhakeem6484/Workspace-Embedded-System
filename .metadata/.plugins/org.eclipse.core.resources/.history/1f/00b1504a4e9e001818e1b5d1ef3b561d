#include"STD_Types.h"
#define F_CPU 12000000
#include<util/delay.h>
#include<avr/io.h>

arr_7seg[]={63,6,91,79,102,109,125,7,127,111};
int main (void)
{
	u16 i;


	DDRA=0XFF;
	DDRD=0XFF;
	DDRB=0XFF;
while(1)
{
	for(i=0;i<10;i++)
	{
		PORTA=arr_7seg[i];
		_delay_ms(1000);


	}



}

return 0;
}

