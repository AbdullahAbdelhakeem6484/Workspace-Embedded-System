#define F_CPU 12000000
#include <avr/io.h>
#include <util/delay.h>

char arr[]={0b00100000,0b01111001,0b01000100,0b01010000,0b00011001,0b00010010,0b00000010,0b01111000,0b00000000,0b00010000};
int main (void)
{
	int i=0;
	DDRA=0xff;

while(1)
{
	for(i=0x00;i<0x0f;i++)
	{
		PORTA=arr[i];
		_delay_ms(1000);
	}
   }
	return 0;
}

